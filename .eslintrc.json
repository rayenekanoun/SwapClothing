{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-mixed-spaces-and-tabs": "off",
    "no-trailing-spaces": "off",
    "eol-last": "off",
    "indent": "off",
    "linebreak-style": "off",
    "semi": "off",
    "quotes": "off"
  }
}
